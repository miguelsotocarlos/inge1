	transfer := WireTransfer for: anAmount.
	depositLeg := WireTransferLeg from: transfer.
	withdrawLeg := WireTransferLeg from: transfer.
	
	transfer setDeposit: (Deposit register: anAmount on: receiver wireTransfer: transfer).
	transfer setWithdrawal: (Withdraw register: anAmount on: sender wireTransfer: transfer). 
