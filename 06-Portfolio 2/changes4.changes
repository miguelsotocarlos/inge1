!Portfolio methodsFor: 'accounts management' stamp: 'HAW 5/25/2019 11:49:20'!
               accountsIncludes: anAccount

	^accounts includes: anAccount ! !
!Portfolio methodsFor: 'accounts management' stamp: 'HAW 5/25/2019 12:05:04'!
                  accountsIsEmpty
	
	^accounts isEmpty ! !
